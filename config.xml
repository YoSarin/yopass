<?xml version='1.0' encoding='utf-8'?>
<widget defaultlocale="en-US" id="io.cordova.YoPass" version="1.0.0" windows-packageVersion="1.1.0.9" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <name>YoPass</name>
    <description>A blank project that uses Apache Cordova to help you build an app that targets multiple mobile platforms: Android, iOS, Windows, and Windows Phone.</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">Apache Cordova Team </author>
    <content src="index.html" />
    <vs:features />
    <preference name="SplashScreen" value="screen" />
    <preference name="windows-target-version" value="10.0" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <icon src="res/icon.png" />
    <platform name="android">
        <splash density="port-xhdpi" src="res/screens/android/splash-port-xhdpi.png" />
    </platform>
    <platform name="ios">
        <splash src="res/screens/ios/Default@2x~universal~anyany.png" />
    </platform>
    <platform name="windows">
        <splash src="res/screens/windows/splashscreen.png" target="SplashScreen" />
    </platform>
    <preference name="KeepRunning" value="True" />
    <preference name="ShowTitle" value="True" />
    <preference name="InAppBrowserStorageEnabled" value="True" />
    <preference name="SuppressesIncrementalRendering" value="True" />
    <vs:platformSpecificValues>
        <vs:platformSpecificWidget id="57917YoSarin.YoPass" platformName="windows" version="1.1.0.8">
            <vs:publisherId>CN=BE6CAD0A-8BA8-4F49-A97C-C3D999B6A33D</vs:publisherId>
            <vs:author>YoSarin</vs:author>
            <vs:name>YoPass</vs:name>
        </vs:platformSpecificWidget>
    </vs:platformSpecificValues>
    <vs:toolsetVersion>6.3.1</vs:toolsetVersion>
    <preference name="android-minSdkVersion" value="14" />
    <engine name="ios" spec="4.2.0" />
    <engine name="windows" spec="4.4.2" />
    <engine name="android" spec="^6.4.0" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-sqlite-storage" spec="2.0.2" />
    <plugin name="cordova-plugin-dialogs" spec="1.3.1" />
    <plugin name="com.verso.cordova.clipboard" spec="~0.1.0" />
    <plugin name="cordova-plugin-secure-storage" spec="~2.6.8" />
    <plugin name="cordova-plugin-fingerprint-aio" spec="~1.3.4" />
    <plugin name="cordova-plugin-add-swift-support" spec="~1.7.0" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
</widget>
